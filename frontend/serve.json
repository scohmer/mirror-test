{
  "rewrites": [
    { "source": "/api/**", "destination": "http://backend:8000/api/**" },
    { "source": "/ws/**", "destination": "http://backend:8000/ws/**" },
    { "source": "/test/ws", "destination": "http://backend:8000/test/ws" }
  ],
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}